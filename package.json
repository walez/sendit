{
  "name": "sendit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx babel src --out-dir dist && npx copyfiles -u 1 src/app/**/*.html src/app/**/*.css src/app/public/**/*.* src/app/views/**/*.* dist/",
    "watch": "babel -w -p ./",
    "debug": "npm run build && npm run lint && npx cross-env NODE_ENV=development nodemon --nolazy ./dist/server.js",
    "lint": "npx eslint --ext=.js ./src",
    "lint-fix": "npx eslint --fix --ext=.js ./src",
    "test": "npx babel src --out-dir dist && mocha ./dist/tests",
    "document": "npx typedoc",
    "start": "node -r dotenv/config ./dist/server.js",
    "start-dev": "npm run build && npm run lint && npx cross-env NODE_ENV=development nodemon -r dotenv/config ./dist/server.js"
  },
  "author": "Wale Martins",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "dotenv": "latest",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "helmet": "^3.14.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "pg": "^7.6.1",
    "swagger-ui-express": "^4.0.1",
    "winston": "^3.1.0",
    "xml": "^1.0.1",
    "xss-clean": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/mocha": "^5.2.5",
    "acorn": "^6.0.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "copyfiles": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.5",
    "typedoc": "^0.13.0"
  }
}